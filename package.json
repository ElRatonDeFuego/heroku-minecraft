{
  "name": "heroku-minecraft",
  "version": "0.0.2",
  "description": "Run Minecraft on Heroku",
  "engines": {
    "node": "14.17.0",
    "npm": "6.14.13"
  },
  "scripts": {
    "build": "tsc --skipLibCheck",
    "build:dev": "tsc --skipLibCheck --sourceMap",
    "clean": "\\rm -rf ./lib",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --fix --ext .ts",
    "prebuild": "npm run clean && npm run lint:fix && npm run prettier",
    "prebuild:dev": "npm run clean && npm run lint:fix && npm run prettier",
    "prettier": "prettier --write './src/**/*.ts' package.json .eslintrc.js",
    "start": "node lib/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jacob Gillespie",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^10.0.0",
    "glob": "^7.1.7",
    "ssh2-sftp-client": "^7.0.0",
    "tslib": "^2.3.0",
    "websocket": "^1.0.8"
  },
  "homepage": "https://github.com/jacobwgillespie/heroku-minecraft",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^15.12.3",
    "@types/ssh2-sftp-client": "^5.3.1",
    "@types/websocket": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.3.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^5.1.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.4"
  }
}
